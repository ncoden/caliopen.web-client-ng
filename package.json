{
  "name": "caliopen-web-client-ng",
  "version": "0.1.0",
  "description": "CaliOpen UI is the user interface for CaliOpen.",
  "homepage": "https://caliopen.org/",
  "private": true,
  "main": "src/app.js",
  "scripts": {
    "build": "gulp",
    "dev": "gulp build watch",
    "release": "gulp release",
    "test": "npm run test-unit && npm run test-functional",
    "test-unit": "karma start test/unit/karma.conf.js --single-run",
    "test-watch": "karma start test/unit/karma.conf.js",
    "test-functional": "NODE_ENV=test npm run build && concurrently --kill-others 'http-server dist/ -p 4200' 'npm run e2e'",
    "server": "http-server dist/ -p 4200",
    "e2e": "protractor ./test/functional/protractor.conf.js",
    "e2e-init": "webdriver-manager update",
    "lint": "eslint src",
    "check-deps": "check-deps -d"
  },
  "author": "CaliOpen <contact@caliopen.org> (https://caliopen.org/)",
  "license": "MIT",
  "dependencies": {},
